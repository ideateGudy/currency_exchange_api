{
  "name": "currency-exchange-api",
  "version": "1.0.0",
  "description": "",
  "license": "ISC",
  "author": "ideategudy",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "nodemon --exec ts-node src/index.ts",
    "devv": "dotenv -e .env -- nodemon --exec ts-node src/index.ts",
    "build": "tsup src/index.ts --format esm --dts --out-dir dist",
    "serve": "node dist/index.js",
    "prisma:init": "npx prisma init --datasource-provider mysql --output ../generated/prisma",
    "prisma:migrate": "npx dotenv -e .env -- prisma migrate dev --name init",
    "prisma:generate": "npx dotenv -e .env -- prisma generate"
  },
  "devDependencies": {
    "@types/express": "^5.0.4",
    "@types/node": "^24.9.1",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.38.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "prisma": "^6.18.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "axios": "^1.12.2",
    "canvas": "^3.2.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0"
  }
}
